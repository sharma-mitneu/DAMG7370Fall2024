{
	"name": "wf_stage_chicago_v2",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "stage_chicago_v1",
						"type": "DatasetReference"
					},
					"name": "sourcesnowflakechicagostage"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "stage_chicago_v2",
						"type": "DatasetReference"
					},
					"name": "sinkStageChicago"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "derivedColumn2"
				},
				{
					"name": "union1"
				},
				{
					"name": "filter1"
				}
			],
			"scriptLines": [
				"parameters{",
				"     DI_WORKFLOW_NAME as string (\"STG_CHICAGO_V2\")",
				"}",
				"source(output(",
				"          CRASH_RECORD_ID as string,",
				"          CRASH_DATE_EST_I as string,",
				"          CRASH_DATE as timestamp,",
				"          POSTED_SPEED_LIMIT as decimal(38,0),",
				"          TRAFFIC_CONTROL_DEVICE as string,",
				"          DEVICE_CONDITION as string,",
				"          WEATHER_CONDITION as string,",
				"          LIGHTING_CONDITION as string,",
				"          FIRST_CRASH_TYPE as string,",
				"          TRAFFICWAY_TYPE as string,",
				"          LANE_CNT as decimal(38,0),",
				"          ALIGNMENT as string,",
				"          ROADWAY_SURFACE_COND as string,",
				"          ROAD_DEFECT as string,",
				"          REPORT_TYPE as string,",
				"          CRASH_TYPE as string,",
				"          INTERSECTION_RELATED_I as string,",
				"          NOT_RIGHT_OF_WAY_I as string,",
				"          HIT_AND_RUN_I as string,",
				"          DAMAGE as string,",
				"          DATE_POLICE_NOTIFIED as timestamp,",
				"          PRIM_CONTRIBUTORY_CAUSE as string,",
				"          SEC_CONTRIBUTORY_CAUSE as string,",
				"          STREET_NO as decimal(38,0),",
				"          STREET_DIRECTION as string,",
				"          STREET_NAME as string,",
				"          BEAT_OF_OCCURRENCE as decimal(38,0),",
				"          PHOTOS_TAKEN_I as string,",
				"          STATEMENTS_TAKEN_I as string,",
				"          DOORING_I as string,",
				"          WORK_ZONE_I as string,",
				"          WORK_ZONE_TYPE as string,",
				"          WORKERS_PRESENT_I as string,",
				"          NUM_UNITS as decimal(38,0),",
				"          MOST_SEVERE_INJURY as string,",
				"          INJURIES_TOTAL as integer,",
				"          INJURIES_FATAL as integer,",
				"          INJURIES_INCAPACITATING as decimal(38,0),",
				"          INJURIES_NON_INCAPACITATING as decimal(38,0),",
				"          INJURIES_REPORTED_NOT_EVIDENT as decimal(38,0),",
				"          INJURIES_NO_INDICATION as decimal(38,0),",
				"          INJURIES_UNKNOWN as decimal(38,0),",
				"          CRASH_HOUR as decimal(38,0),",
				"          CRASH_DAY_OF_WEEK as decimal(38,0),",
				"          CRASH_MONTH as decimal(38,0),",
				"          LATITUDE as double,",
				"          LONGITUDE as double,",
				"          LOCATION as string,",
				"          CITY as string,",
				"          COUNTRY as string,",
				"          DI_SOURCE_NAME as string,",
				"          DI_CREATED_DATE as date,",
				"          DI_WORKFLOW_NAME as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table') ~> sourcesnowflakechicagostage",
				"sourcesnowflakechicagostage derive(CRASH_DATE = toDate(CRASH_DATE),",
				"          CRASH_TIME = substring(toString(CRASH_DATE), 11, 8),",
				"          PRIM_CONTRIBUTORY_CAUSE = upper(trim(PRIM_CONTRIBUTORY_CAUSE)),",
				"          SEC_CONTRIBUTORY_CAUSE = upper(trim(SEC_CONTRIBUTORY_CAUSE)),",
				"          STREET_NAME = upper(trim(STREET_NAME)),",
				"          INJURIES_TOTAL = iif(isNull(INJURIES_FATAL), 0, INJURIES_FATAL),",
				"          INJURIES_FATAL = iif(isNull(INJURIES_FATAL), 0, INJURIES_FATAL),",
				"          LATITUDE = iif(isNull(LATITUDE), toDouble(0), LATITUDE),",
				"          LONGITUDE = iif(isNull(LONGITUDE), toDouble(0), LONGITUDE),",
				"          CONTRIBUTING_FACTOR = upper(trim(PRIM_CONTRIBUTORY_CAUSE))) ~> derivedColumn1",
				"derivedColumn1 derive(CONTRIBUTING_FACTOR = upper(trim(SEC_CONTRIBUTORY_CAUSE))) ~> derivedColumn2",
				"derivedColumn2, derivedColumn1 union(byName: true)~> union1",
				"union1 filter(!isNull(CONTRIBUTING_FACTOR)) ~> filter1",
				"filter1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          CRASH_RECORD_ID as string,",
				"          CRASH_DATE as string,",
				"          CRASH_TIME as string,",
				"          PRIM_CONTRIBUTORY_CAUSE as string,",
				"          SEC_CONTRIBUTORY_CAUSE as string,",
				"          STREET_NAME as string,",
				"          INJURIES_TOTAL as string,",
				"          INJURIES_FATAL as string,",
				"          LATITUDE as string,",
				"          LONGITUDE as string,",
				"          CONTRIBUTING_FACTOR as string,",
				"          CITY as string,",
				"          COUNTRY as string,",
				"          DI_SOURCE_NAME as string,",
				"          DI_CREATED_DATE as string,",
				"          DI_WORKFLOW_NAME as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     stageInsert: true,",
				"     mapColumn(",
				"          CRASH_RECORD_ID,",
				"          CRASH_DATE,",
				"          CRASH_TIME,",
				"          PRIM_CONTRIBUTORY_CAUSE,",
				"          SEC_CONTRIBUTORY_CAUSE,",
				"          STREET_NAME,",
				"          INJURIES_TOTAL,",
				"          INJURIES_FATAL,",
				"          LATITUDE,",
				"          LONGITUDE,",
				"          CONTRIBUTING_FACTOR,",
				"          CITY,",
				"          COUNTRY,",
				"          DI_SOURCE_NAME,",
				"          DI_CREATED_DATE,",
				"          DI_WORKFLOW_NAME",
				"     )) ~> sinkStageChicago"
			]
		}
	}
}